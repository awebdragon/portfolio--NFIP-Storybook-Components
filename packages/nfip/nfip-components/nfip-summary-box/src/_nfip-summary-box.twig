{% embed '@nfip-components/nfip-section/nfip-section.twig' %}
  {% block sectionContent %}
    <div>
    
    {% embed '@nfip-components/nfip-summary-box/src/nfip-summary-box.twig' %}
    {% block summaryBody %}
    {{summary_body}}
    {% include '@nfip-components/nfip-button/src/_button.twig' with {
      'button_type': 'default',
      'button_markup': 'link',
      'button_text': summary_cta.link_text,
      'button_url': summary_cta.url,
      'button_target': '_self',
      'button_style': 'normal'
    } %}
    </div>
    {% endblock %}
    {% endembed %}
    
  {% endblock %}
{% endembed %}

{% embed '@nfip-components/nfip-section/nfip-section.twig' %}
  {% block sectionContent %}
    <div class="desktop:grid-col-10 desktop:grid-offset-1">
    
    {% embed '@nfip-components/nfip-summary-box/src/nfip-summary-box.twig' %}
    {% block summaryBody %}
    {{summary_body}}
    {% include '@nfip-components/nfip-button/src/_button.twig' with {
      'button_type': 'default',
      'button_markup': 'link',
      'button_text': summary_cta.link_text,
      'button_url': summary_cta.url,
      'button_target': '_self',
      'button_style': 'normal'
    } %}
    </div>
    {% endblock %}
    {% endembed %}
    
  {% endblock %}
{% endembed %}