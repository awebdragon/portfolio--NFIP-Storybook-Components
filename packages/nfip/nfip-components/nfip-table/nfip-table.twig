
{% embed '@nfip-components/nfip-section/nfip-section.twig' %}
  {% block sectionContent %}
  <div class="usa-table-container--scrollable" tabindex="0">
    <table class="usa-table nfip-table width-full"{% if caption and hiddenCaption %} aria-label="{{ caption }}"{% endif %}>
    {% if caption and not hiddenCaption %}
    <caption>{{ caption }}</caption>
    {% endif %}
    {{table}}
    </table>
  </div>
  {% endblock %}
{% endembed %}