{% set backgroundColor = 'white' %}
{% embed '@nfip-components/nfip-section/nfip-section.twig' with {  paddingTop: 3,paddingBottom: 0 } %}
  {% block sectionContent %}
    <div class="grid-row">
      <div class="desktop:grid-col-10 desktop:grid-offset-1">
        <h6>On this page:</h6>
        <ul class="nfip-jumplinks">
        {% for link in jumpLinks %}
          <li><a href="#{{link|replace({' ': '-'})|replace({'/[^a-zA-Z0-9\-]/': ''})|lower }}">{{link}}</a></li>
        {% endfor %}
        </ul>
      </div>
    </div>
  {% endblock %}
{% endembed %}
