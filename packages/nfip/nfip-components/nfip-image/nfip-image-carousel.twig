
{% set options = {
  show_nav: true
}
%}
{% embed '@nfip-components/nfip-section/nfip-section.twig' with { containerWidth: 'desktop' } %}
  {% block sectionContent %}
  {% embed '@nfip-components/nfip-carousel/nfip-carousel.twig' with { options: options, carousel_aria_name: "Quote Carousel", nav_position: nav_position } %}
    {% block carouselSlides %}
      {% for image in images %}
      <div data-slide-number="{{ loop.index }}" tabindex="0" role="group" aria-roledescription="slide">
      {% include '@nfip-components/nfip-image/nfip-image.twig' with {
        image: image.image,
        image_alt: image.image_alt,
        caption: image.caption
      } %}
      </div>
      {% endfor %}
    {% endblock %}
  {% endembed %}
  {% endblock %}
{% endembed %}
