  {% embed '@nfip-components/nfip-section/nfip-section.twig' %}
    {% block sectionContent %}
    {% embed '@nfip-components/nfip-carousel/nfip-carousel.twig' with { options: quoteOptions, carousel_aria_name: "Quote Carousel", nav_position: nav_position } %}
      {% block carouselSlides %}
        {% for quote in quotes %}
          <div data-slide-number="{{ loop.index }}" tabindex="0" role="group" aria-roledescription="slide">
          {% include '@nfip-components/nfip-quote/src/nfip-quote.twig' with quote %}
          </div>
        {% endfor %}
      {% endblock %}
    {% endembed %}

    {% endblock %}
  {% endembed %}

  {% embed '@nfip-components/nfip-section/nfip-section.twig' %}
    {% block sectionContent %}
      {% include '@nfip-components/nfip-quote/src/nfip-quote.twig' with {
        quote: quotes.0.quote,
        name: quotes.0.name,
        quote_title: quotes.0.quote_title
      } %}
    {% endblock %}
  {% endembed %}

  {% embed '@nfip-components/nfip-section/nfip-section.twig' %}
    {% block sectionContent %}
    <div class="usa-table-container--scrollable" tabindex="0">
    
